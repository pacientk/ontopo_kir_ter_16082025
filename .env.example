# News API Configuration
EXPO_PUBLIC_NEWS_API_KEY=your_news_api_key_here

# AI Provider Configuration (openai, gemini, mock)
EXPO_PUBLIC_AI_PROVIDER=gemini

# OpenAI API Key (only needed if using OpenAI)
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Key (FREE up to 60 requests/minute)
# Get free key at: https://makersuite.google.com/app/apikey
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# Use mock data instead of real AI calls (true/false)
EXPO_PUBLIC_USE_MOCK_DATA=true
